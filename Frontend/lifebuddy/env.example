# Frontend Environment Variables
# Copy this to .env.local for development

# Backend URLs (Railway is priority 1, Render is fallback)
VITE_RAILWAY_URL=https://lifebuddy-backend-production.up.railway.app
VITE_RENDER_URL=https://lifebuddy.onrender.com

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info